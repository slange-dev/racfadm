)Body window(77,06) expand(\\)
%Command ===>_zcmd
+
+ICETOOL Report PDS%===>_racipds                                       +
+DSMON   Report PDS%===>_racdpds                                       +
+
%ENTER+to continue or%F3+to cancel
)Init
  &ZCMD    = ' '
  .cursor  = racipds
  .HELP    = #RACRPDS
  &ZWINTTL = 'RACFADM - Report Generation PDS Datasets'
  if (&racipds = '')
    &racipds = '''&zuser..RACFADM.ICETOOL'''
  if (&racdpds = '')
    &racdpds = '''&zuser..RACFADM.DSMON'''
)Proc
  ver (&racipds,nb,dsname)
  ver (&racdpds,nb,dsname)
  vput (racipds racdpds) profile
  &ZWINTTL = ''
)End
